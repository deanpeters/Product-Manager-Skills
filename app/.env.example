# Copy this file to .env and add one or more API keys.
# The app loads these values automatically on startup.
# API keys are environment-variable only (no in-app key entry).

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...
ANTHROPIC_MODEL=claude-sonnet-4-6
ANTHROPIC_MODELS=claude-haiku-4-5-20251001,claude-sonnet-4-6

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
OPENAI_MODELS=gpt-4o-mini,gpt-4o

# Ollama (local)
OLLAMA_ENABLED=1
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_API_KEY=ollama
OLLAMA_MODEL=qwen2.5:latest
OLLAMA_MODELS=qwen2.5:latest,llama3.2:latest
